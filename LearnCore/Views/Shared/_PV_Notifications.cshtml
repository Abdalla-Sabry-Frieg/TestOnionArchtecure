
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.js"></script>

@if (TempData["SuccessData"] != null)
{
    <h3 style="color:brown;"> @TempData["SuccessData"]</h3>

    <script type="text/javascript">
        toastr.success('@TempData["SuccessData"]', 'SuccessData')
    </script>
}
   

@if (TempData["ErorrData"] != null)
{
    <h3 style="color:brown;"> @TempData["ErorrData"]</h3>

    <script type="text/javascript">
        toastr.error('@TempData["ErorrData"]', 'ErorrData')


    </script>
}